    "import math\n",
    "\n",
    "def norm_vector(v):\n",
    "    dot_product = sum(i*i for i in v)\n",
    "    return math.sqrt(dot_product)\n",
    "\n",
    "vector = (-1, -2, 3, 4, 5)\n",
    "result = norm_vector(vector)\n",
    "print(result)\n"
   ]
